{{#if model.immature}}
<h4>{{t 'block.immature'}}</h4>

<div class="table-responsive">
  <table class="table table-condensed table-striped">
    <thead>
      <tr>
        <th>{{t 'block.col1'}}</th>
        <th>{{t 'block.col2'}}</th>
        <th>{{t 'block.col3'}}</th>
        <th>{{t 'block.col4'}}</th>
        <th>{{t 'block.col5'}}</th>
      </tr>
    </thead>
    <tbody>
      {{#each model.immature as |block|}}
        {{partial "blocks/block"}}
      {{/each}}
    </tbody>
  </table>
</div>
{{else}}
<h3>{{t 'block.immature'}}</h3>
{{/if}}
